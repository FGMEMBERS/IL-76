<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <chase-distance-m type="double" archive="y"> -70 </chase-distance-m>

  <view n="100">
    <name>Co Pilote</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">   0.61156 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  -1.89655 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -19.44196 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> -5 </pitch-offset-deg>
      <default-field-of-view-deg> 80 </default-field-of-view-deg>
    </config>
  </view>

  <view n="101">
    <name>Cabin</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">   0.0 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  -1.7 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -18.5 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> -5 </pitch-offset-deg>
      <default-field-of-view-deg> 80 </default-field-of-view-deg>
    </config>
  </view>
  
  <view n="102">
    <name>Engineer</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">   0.84172 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  -1.89604 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -17.59156 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> 0 </pitch-offset-deg>
      <heading-offset-deg type="double"> -90 </heading-offset-deg>
      <default-field-of-view-deg> 80 </default-field-of-view-deg>
    </config>
  </view>
  
  <view n="103">
    <name>Exterior tail View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">  0.00000 </x-offset-m> <!--Right-->
      <y-offset-m archive="y">  8.01152 </y-offset-m> <!--Up-->
      <z-offset-m archive="y"> 15.75844 </z-offset-m> <!--Back-->
      <pitch-offset-deg> -5 </pitch-offset-deg>
      <default-field-of-view-deg type="double"> 55 </default-field-of-view-deg>
    </config>
  </view>
  
  <view n="0">
    <internal archive="y">true</internal>
    <config>
      <x-offset-m archive="y">  -0.61156 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  -1.89655 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -19.44196 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> -5 </pitch-offset-deg>
      <default-field-of-view-deg> 80 </default-field-of-view-deg>
    </config>
  </view>

  <view n="1">
